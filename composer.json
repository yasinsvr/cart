{
    "name": "yasinsvr/cart",
    "description": "Laravel shopping cart",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "shopping cart", "cart"],
  
    "authors": [
      {
        "name": "Yasin Zolfaghari",
        "email": "yasin.zolfaghari.dev@gmail.com"
      }
    ],
  
    "require": {
      "php": "^8.1",
      "illuminate/support": "^10.0 || ^11.0 || ^12.0",
      "illuminate/validation": "^10.0 || ^11.0 || ^12.0",
      "illuminate/translation": "^10.0 || ^11.0 || ^12.0"
    },
  
    "require-dev": {
      "mockery/mockery": "^1.4.2",
      "phpunit/phpunit": "^10.0",
      "symfony/var-dumper": "^5.1",
      "imanghafoori/eloquent-mockery": "dev-main" 
    },
  
    "autoload": {
      "psr-4": {
        "YasinSVR\\Cart\\": "src/"
      }
    },
  
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/",
        "Tests\\Helpers\\": "tests/helpers/"
      }
    },
  
    "minimum-stability": "stable",
    "prefer-stable": true,

    "extra": {
      "laravel": {
        "providers": [
          "YasinSVR\\Cart\\CartServiceProvider"
        ],
        "aliases": {
          "Cart": "YasinSVR\\Cart\\Facades\\CartFacade"
        }
      }
    }
  }
  