{
  "name": "yasinsvr/cart",
  "description": "Laravel Shopping cart",
  "keywords": [
    "laravel",
    "shopping cart",
    "cart"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Yasin Zolfaghari",
      "email": "yasin.zolfaghari.dev@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "illuminate/support": "10.0.*|^11.0|^12.0",
    "illuminate/validation": "10.0.*|^11.0|^12.0",
    "illuminate/translation": "10.0.*|^11.0|^12.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.4.2",
    "phpunit/phpunit": "^9.0",
    "symfony/var-dumper": "5.1.5.*@dev",
    "imanghafoori/eloquent-mockery": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "YasinSVR\\Cart\\": "src/"
    },
    "files": [
      "src/Tools/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Tests\\Helpers\\": "tests/helpers/"
    },
    "files": [
      "tests/helpers/functions.php"
    ]
  },
  "minimum-stability": "dev",
  "extra": {
    "laravel": {
      "providers": [
        "YasinSVR\\Cart\\CartServiceProvider"
      ],
      "aliases": {
        "Cart": "YasinSVR\\Cart\\Facades\\CartFacade"
      }
    }
  }
}
